/* rexx */                          
RC=ISFCALLS('ON')                   
address SDSF "ISFEXEC '/d iplinfo'" 
/*********************************/ 
/*       VIEW A ULOG             */ 
/*********************************/ 
do ix=1 to isfulog.0                
  say "isfulog."ix "is" isfulog.ix  
end                                 
RC=ISFCALLS('OFF')                  
exit                                
